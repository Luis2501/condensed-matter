   "source": [
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "plt.style.use(\"science.mplstyle\")\n",
    "\n",
    "X, Y = np.meshgrid(np.arange(-2,2, 0.2), np.arange(-2,2, 0.2))\n",
    "\n",
    "fig = plt.figure()\n",
    "ax = fig.add_subplot(projection='3d')\n",
    "\n",
    "ax.grid(False)\n",
    "ax.plot_surface(X, Y, X**2 + Y**2)"
   ]
